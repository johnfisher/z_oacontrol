	zZX1900BASEVPDV7 OBJECT-TYPE
   		SYNTAX DisplayString
   		ACCESS read-only
   	 	STATUS     mandatory
   		DESCRIPTION
    	    		"The Reset Counter."
    		::= { znyxPsMibZX1900BASE 17 }

	zZX1900BASEVPDV8 OBJECT-TYPE
   		SYNTAX DisplayString
